generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id          String  @id
  traqId      String  @unique
  name        String
  displayName String
  iconFileId  String
  session     Session @relation(fields: [sessionId], references: [id])
  sessionId   String  @unique
}

model Session {
  id           String   @id
  refreshToken String
  expiresAt    DateTime
  user         User?
}
