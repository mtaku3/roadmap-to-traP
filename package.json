{
  "name": "roadmap-to-trap",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "src/modules/infra/prisma/schema.prisma",
    "seed": "tsx --env-file=.env.local src/modules/infra/prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "og:traq": "openapi-generator-cli generate -g typescript-axios -i https://raw.githubusercontent.com/traPtitech/traQ/master/docs/v3-api.yaml -o ./src/modules/external/traq/ --api-package api --model-package model --additional-properties withSeparateModelsAndApi=true",
    "og:knoq": "openapi-generator-cli generate -g typescript-axios -i https://raw.githubusercontent.com/traPtitech/knoQ/master/docs/swagger.yaml -o ./src/modules/external/knoq/ --api-package api --model-package model --additional-properties withSeparateModelsAndApi=true"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json,md}": "prettier --write"
  },
  "dependencies": {
    "@mantine/core": "^7.9.1",
    "@mantine/hooks": "^7.9.1",
    "@prisma/client": "^5.13.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.35.1",
    "@trpc/client": "^11.0.0-rc.366",
    "@trpc/next": "^11.0.0-rc.366",
    "@trpc/react-query": "^11.0.0-rc.366",
    "@trpc/server": "^11.0.0-rc.366",
    "awilix": "^10.0.2",
    "axios": "^1.6.8",
    "axios-cache-interceptor": "^1.5.2",
    "jose": "^5.2.4",
    "jotai": "^2.8.0",
    "next": "14.2.3",
    "openid-client": "^5.6.5",
    "pluralize": "^8.0.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.2.1",
    "superjson": "^2.2.1",
    "tsx": "^4.10.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/node": "^20",
    "@types/pluralize": "^0.0.33",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "plop": "^4.0.1",
    "postcss": "^8",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.5",
    "prisma": "^5.13.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
